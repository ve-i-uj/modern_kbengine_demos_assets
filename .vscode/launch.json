{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Python: Current File",
            "type": "python",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal",
            "cwd": "${fileDirname}",
            "env": {
                "PYTHONPATH": "${workspaceFolder}/scripts/base:${workspaceFolder}/scripts/server_common:${workspaceFolder}/scripts/user_type:${workspaceFolder}/scripts/common:${workspaceFolder}/scripts/data"
            }
        }
    ]
}
